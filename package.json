{
  "name": "sapops",
  "version": "0.3.1",
  "license": "MIT",
  "scripts": {
    "build": "nx run-many --target=build",
    "make:secrets": "CDS_ENV=hybrid cds env get requires.db.credentials --resolve-bindings > .cds/requires/db/credentials",
    "init:data": " CDS_ENV=hybrid npx nx g datagen",
    "init:sqlite": "cds deploy --to sqlite",
    "nx:publish": "nx run-many --target=publish",
    "preversion": "nx affected --target=version --base=v${npm_old_version} ${npm_new_version} --workspaces-update=false",
    "version": "npm install --ignore-scripts --package-lock-only --fund=false --audit=false && git add -A "
  },
  "private": true,
  "dependencies": {
    "@cap-js/cds-types": "^0.6.5",
    "@cap-js/hana": "^1.1.1",
    "@cap-js/sqlite": "^1.7.3",
    "@commander-js/extra-typings": "^12",
    "@fastify/deepmerge": "^2",
    "@sap/cds": "^8",
    "@sap/hana-client": "^2.21.28",
    "commander": "^12",
    "tslib": "^2.6.3"
  },
  "devDependencies": {
    "@nx/devkit": "19.6.2",
    "@nx/eslint": "19.6.2",
    "@nx/eslint-plugin": "19.6.2",
    "@nx/jest": "19.6.2",
    "@nx/js": "19.6.2",
    "@nx/plugin": "19.6.2",
    "@nx/workspace": "19.6.2",
    "@sap/cds-dk": "^8",
    "@sap/eslint-plugin-cds": "^3",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.3.12",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/jest": "29.5.12",
    "@types/node": "18.19.9",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "json2fs": "^0.1.4",
    "nx": "19.6.2",
    "prettier": "^2.6.2",
    "set-shebang": "^0.1.12",
    "sqlite3": "^5.1.7",
    "ts-jest": "29.1.5",
    "ts-node": "^10.9.1",
    "type-fest": "^4",
    "typescript": "^5.5.4"
  },
  "workspaces": [
    "packages/**"
  ],
  "cds": {
    "log": {
      "levels": {
        "db|pool": "warn"
      }
    }
  },
  "nx:": {
    "targets": {}
  }
}
