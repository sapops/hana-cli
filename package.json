{
  "name": "sapops",
  "version": "0.3.1",
  "license": "MIT",
  "scripts": {
    "build": "nx run-many --target=postbuild",
    "lint": "nx run-many --target=lint",
    "init:data": " CDS_ENV=hybrid npx nx g datagen",
    "init:sqlite": "CDS_ENV=mock cds deploy",
    "init:types": "npx @cap-js/cds-typer db/model.cds --outputDirectory db/generated/types",
    "publish:login": "npm login --registry http://verdaccio:4873",
    "publish:local": "NPM_CONFIG_REGISTRY=http://verdaccio:4873 nx release publish",
    "release:patch": "nx release patch --skip-publish"
  },
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^1.1.1",
    "@cap-js/sqlite": "^1.7.3",
    "@commander-js/extra-typings": "^12",
    "@fastify/deepmerge": "^2",
    "@sap/cds": "^8",
    "@sap/hana-client": "^2.21.28",
    "commander": "^12",
    "tslib": "^2.6.3"
  },
  "devDependencies": {
    "@cap-js/cds-typer": ">=0.1",
    "@cap-js/cds-types": "^0.6.5",
    "@eslint/eslintrc": "^2.1.1",
    "@eslint/js": "~8.57.0",
    "@nx/devkit": "19.6.3",
    "@nx/eslint": "19.6.3",
    "@nx/eslint-plugin": "19.6.3",
    "@nx/jest": "19.6.3",
    "@nx/js": "19.6.3",
    "@nx/plugin": "19.6.3",
    "@nx/workspace": "19.6.3",
    "@sap/cds-dk": "^8",
    "@sap/eslint-plugin-cds": "^3.0.4",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.3.12",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "29.5.12",
    "@types/node": "18.19.9",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-formatter-stylish": "^8.40.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-environment-node": "^29.7.0",
    "json2fs": "^0.1.4",
    "nx": "19.6.3",
    "prettier": "^2.6.2",
    "set-shebang": "^0.1.12",
    "sqlite3": "^5.1.7",
    "ts-jest": "29.1.5",
    "ts-node": "^10.9.1",
    "type-fest": "^4",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.2.0",
    "verdaccio": "^5.0.4"
  },
  "workspaces": [
    "packages/**"
  ],
  "cds": {
    "log": {
      "levels": {
        "db|pool": "warn"
      }
    }
  },
  "nx:": {
    "targets": {}
  },
  "imports": {
    "#cds-models/*": "./@cds-models/*/index.js"
  },
  "nx": {
    "includedScripts": []
  }
}
