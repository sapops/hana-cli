{
  "name": "hana2cds",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/hana2cds/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/packages/hana2cds",
        "main": "packages/hana2cds/src/index.ts",
        "tsConfig": "packages/hana2cds/tsconfig.lib.json",
        "assets": [
          "packages/hana2cds/*.md",
          "packages/hana2cds/**/*.csn"
        ],
        "buildableProjectDepsInPackageJsonType":"dependencies"
      },
      "dependsOn": [
        "build-cds-csn",
        "build-cds-types"
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/hana2cds/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "packages/hana2cds/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "build-cds-csn": {
      "executor": "nx:run-commands",
      "inputs": [
        "cds"
      ],
      "outputs": [
        "{projectRoot}/src/cds/public.csn"
      ],
      "options": {
        "command": "npx cds compile src/cds/public.cds --dest src/cds/public.csn",
        "cwd": "packages/hana2cds/src/cds"
      }
    },
    "build-cds-types": {
      "executor": "nx:run-commands",
      "inputs": [
        "cds"
      ],
      "outputs": [
        "{projectRoot}/src/cds/other.ts"
      ],
      "options": {
        "command": "npx cds2types --cds ./src/cds/public.cds --output ./src/cds/",
        "cwd": "packages/hana2cds/src/cds"
      }
    },
    "version": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm version",
        "cwd": "packages/hana2cds"
      }
    }
  },
  "tags": []
}